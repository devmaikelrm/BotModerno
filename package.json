{
  "name": "cubamodel-bot",
  "version": "2.2.0",
  "description": "Telegram bot with web panel for CubaModel - Cloudflare Workers deployment",
  "main": "src/worker.js",
  "type": "module",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "dev:panel": "cd web-panel && npm run dev",
    "build:panel": "cd web-panel && npm run build",
    "start": "wrangler dev",
    "test": "jest",
    "test:e2e": "playwright test",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.57.4",
    "dotenv": "^17.2.2",
    "p-limit": "^7.1.1",
    "papaparse": "^5.5.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250909.0",
    "@eslint/js": "^9.35.0",
    "@playwright/test": "^1.40.0",
    "@types/node": "^20.0.0",
    "eslint": "^9.35.0",
    "jest": "^29.7.0",
    "typescript": "^5.4.0",
    "wrangler": "^4.34.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
